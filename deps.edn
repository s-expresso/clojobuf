{:paths ["src", "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.11.1"}
        org.clojure/clojurescript {:mvn/version "1.11.60"}
        com.google.javascript/closure-compiler-unshaded {:mvn/version "v20230103"}
        metosin/malli {:mvn/version "0.12.0"}
        s-expresso/rubberbuf {:git/url "https://github.com/s-expresso/rubberbuf.git"
                              :git/sha "371021dafbc3065aa1b4e8c3396b02d064cba1b5"
                              :git/tag "v0.1.7"}
        s-expresso/clojobuf-codec {:git/url "https://github.com/s-expresso/clojobuf-codec.git"
                                   :git/sha "5e15843a54671aff57da1eb7bb23adaafff1fe54"
                                   :git/tag "v0.1.4"}}
 :aliases {:test {:extra-paths ["test"]
                  ;:patterns [".*"]
                  :extra-deps {io.github.cognitect-labs/test-runner
                               {:git/url "https://github.com/cognitect-labs/test-runner.git"
                                :sha "9e35c979860c75555adaff7600070c60004a0f44"}}
                  :main-opts ["-m" "cognitect.test-runner"]
                  :exec-fn cognitect.test-runner.api/test}
           ; clojure -M:test:cljs to run cljs tests
           :cljs {:extra-deps {olical/cljs-test-runner {:mvn/version "3.8.0"}}
                  :main-opts ["-m" "cljs-test-runner.main"]}
           :shadow {:extra-paths ["test"]
                    :extra-deps {thheller/shadow-cljs {:mvn/version "2.20.20"}}}}}
